@model WebSave.Models.FileViewModel.AddViewModel

<form asp-action="Add" asp-controller="File" asp-route-id="@ViewData["FileTypeId"]" method="post">
    <div>
        <label asp-for="FileName"></label>
        <input autocomplete="off" asp-for="FileName" />
    </div>
    <div>
        <label asp-for="HttpUrl"></label>
        <input autocomplete="off" asp-for="HttpUrl" />
    </div>
    <div>
        <input class="btn btn-danger" type="submit" value="添加" />
    </div>
</form>